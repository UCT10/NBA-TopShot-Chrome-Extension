{
    "name": "NBA Top Shot Chrome Extension",
    "version": "1.0",
    "description": "Sort pricing by ascending ask rather than serial number to find deals",
    "manifest_version": 2,
    "content_scripts": [{
        "js": ["content.js", "assets/scripts/jquery-3.2.1.min.js", "assets/scripts/bootstrap.min.js" ],
        "css": ["styles.css", "assets/scripts/bootstrap.min.css"],
        "matches": ["https://www.nbatopshot.com/listings/p2p/*"]
    }],
    "web_accessible_resources": [
        "assets/*"
    ]
  }